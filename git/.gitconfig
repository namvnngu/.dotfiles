[user]
  name  = Nam Nguyen
  email = nguyenvietnam2401@gmail.com
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore_global
[rerere]
  enabled    = true
  autoUpdate = true
[branch]
  sort = -committerdate
[maintenance]
  auto     = false
  strategy = incremental
[fetch]
  writeCommitGraph = true
[alias]
  logg     = log --decorate --oneline --graph
  logga    = log --all --decorate --oneline --graph
  logol    = log --decorate --oneline
  logola   = log --all --decorate --oneline
  cbranch  = rev-parse --abbrev-ref HEAD
  conflict = diff --name-only --diff-filter=U --relative
